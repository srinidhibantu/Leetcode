class Solution {
    public int minimumDeleteSum(String s1, String s2) {
     int m = s1.length();
     int n = s2.length();
     int[][] dp = new int[m + 1][n + 1];
     for(int j = 1; j <= n; j++){
        int left = dp[0][j - 1];
        dp[0][j] = left + s2.charAt(j - 1);
     }  
    for(int i = 1; i <= m; i++){
        int top = dp[i - 1][0];
        dp[i][0] = top + s1.charAt(i - 1);
      }
     for(int i = 1; i <= m; i++){
       for(int j = 1; j <= n; j++){
        if(s1.charAt(i - 1) == s2.charAt(j - 1)){
            dp[i][j] = dp[i - 1][j - 1];
        }else{
        int left = dp[i][j - 1] + s2.charAt(j - 1);
        int top = dp[i - 1][j] + s1.charAt(i -1); 
        dp[i][j] = Math.min(left, top);  
         }
       }
        }
     return dp[m][n];
    }
}
